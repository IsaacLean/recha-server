{
  "name": "recha-server",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "setup": "yarn && yarn run flow-typed install",
    "build": "webpack --env production",
    "build:debug": "node --inspect-brk node_modules/.bin/webpack --env production",
    "build:dev": "webpack --env development",
    "build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development",
    "build:dev:stats": "yarn run build:dev -- --json > stats.development.json",
    "build:stats": "yarn run build -- --json > stats.production.json",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "format": "prettier --write --parser=flow --print-width=120 --semi=false --single-quote \"{config,src}/**/*.{js,jsx}\"",
    "lint": "eslint --cache --ext .js \"{config,src}/**\"",
    "start": "node build/production/server.js",
    "start:debug": "node --inspect-brk build/production/server.js",
    "start:dev": "node build/development/server.js",
    "start:dev:debug": "node --inspect-brk build/development/server.js",
    "start:dev:watch": "nodemon --watch webpack.config.js --watch config --watch src --exec \"yarn run build:dev && yarn run start:dev\"",
    "start:dev:watch:debug": "nodemon --watch webpack.config.js --watch config --watch src --exec \"yarn run build:dev && yarn run start:dev:debug\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacLean/recha-server.git"
  },
  "author": "Matthew Lean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IsaacLean/recha-server/issues"
  },
  "homepage": "https://github.com/IsaacLean/recha-server#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-fetch": "^3.0.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "moment": "^2.23.0",
    "mongoose": "^5.4.4",
    "source-map-support": "^0.5.10",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-prettier": "^3.0.1",
    "flow-bin": "^0.90.0",
    "flow-typed": "^2.5.1",
    "flow-webpack-plugin": "^1.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "1.15.3",
    "supertest": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
